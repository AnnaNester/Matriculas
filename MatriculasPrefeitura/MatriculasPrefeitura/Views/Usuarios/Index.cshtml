@model IEnumerable<MatriculasPrefeitura.Models.Usuario>

@{
    ViewBag.Title = "Index";
}

<h2>Gerenciamento de Usuários</h2>

<p>
    @Html.ActionLink("Cadastrar Usuário", "CadastrarUsuario")
</p>
<table class="table table-hover table-stripped">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Login)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Senha)
        </th>
    </tr>

    @foreach (var usu in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelUsu => usu.Login)
            </td>
            <td>
                @Html.DisplayFor(modelUsu => usu.Senha)
            </td>
        </tr>
    }

</table>

