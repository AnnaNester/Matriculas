@model MatriculasPrefeitura.Models.Curso
@{
    Layout = null;
}

<h2>Turma</h2>

<table style="margin-left: 30px">
    <tr>
        <td colspan="2">
            @foreach(var aluno in Model.matriculas)
            {
                <b>Matrícula: </b> @aluno.numMatricula
                <b>Nome: </b> @aluno.AlunoMatriculado
            }
        </td>
    </tr>
</table>

